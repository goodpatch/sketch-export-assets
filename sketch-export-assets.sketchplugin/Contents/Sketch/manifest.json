{
    "name": "Sketch Export Assets",
    "description": "Plugins to export assets to different platforms",
    "author": "Geert Wille",
    "homepage": "https://github.com/GeertWille/sketch-export-assets",
    "version": 2.18,
    "identifier": "com.geertwille.sketch.export-assets",
    "compatibleVersion": 3,
    "bundleVersion": 1,
    "commands":
        [
            {
                "name": "Export iOS Assets",
                "identifier": "export-assets-ios",
                "shortcut": "ctrl alt shift 3",
                "script": "Export iOS Assets.sketchscript"
            },
            {
                "name": "Export Android Assets(drawable-)",
                "identifier": "export-assets-android-drawable",
                "shortcut": "ctrl alt shift 1",
                "script": "Export Android Assets Drawable.sketchscript"
            },
            {
                "name": "Export Android Assets(mipmap-)",
                "identifier": "export-assets-android-mipmap",
                "shortcut": "ctrl alt shift 2",
                "script": "Export Android Assets Mipmap.sketchscript"
            },
            {
                "name": "Export WindowsPhone Assets",
                "identifier": "export-assets-wp",
                "shortcut": "ctrl alt shift 4",
                "script": "Export WindowsPhone Assets.sketchscript"
            }
        ],
    "menu":
        {
            "items":
            [
                "export-assets-android-drawable",
                "export-assets-android-mipmap",
                "export-assets-ios",
                "export-assets-wp"
            ]
        }
}